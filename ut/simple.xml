<service name="simple">
	<record name="Address">
		<attr name="country" type="str" />
		<attr name="city" type="str" />
		<attr name="street" type="str" />
		<attr name="num" type="int32" />
	</record>

	<func name="get_address" type="Address">
	</func>
	
	<class name="Person">
		<attr name="name" type="str" />
		<attr name="address" type="Address" />
		
		<method name="marry" type="void">
			<arg name="spouse" type="Person"/>
		</method>

		<method name="give_birth" type="Person">
			<arg name="name" type="str"/>
		</method>

		<method name="get_children" type="list[Person]" />

	</class>
	
	<const name="pi" type="float" value="3.1415926535"/>
	
</service>
