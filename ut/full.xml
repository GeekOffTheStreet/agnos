<service name="full_ut">
	<const name="pi" type="float" value="3.1415926535"/>
	
	<enum name="Sex">
		<member name="MALE" value="3"/>
		<member name="FEMALE"/>
	</enum>

	<enum name="SexualOrientation">
		<member name="MALE" />
		<member name="FEMALE" />
		<member name="BOTH" />
		<member name="NONE" />
	</enum>
	
	<exception name="MarriageViolation">
		<attr name="message" type="str"/>
		<attr name="person" type="Person"/>
	</exception>

	<class name="Person">
		<attr name="given_name" type="str"/>
		<attr name="surname" type="str"/>
		<attr name="date_of_birth" type="date"/>
		<attr name="sex" type="Sex" />
		<attr name="orientation" type="SexualOrientation" />
		
		<attr name="father" type="Person" access="get"/>
		<attr name="mother" type="Person" access="get"/>
		<attr name="spouse" type="Person" access="get"/>
		<attr name="children" type="list[Person]" access="get"/>
		
		<method name="marry" type="void">
			<arg name="other" type="Person"/>
		</method>

		<method name="divorce" type="void">
		</method>
		
		<method name="give_birth" type="Person">
			<arg name="given_name" type="str"/>
		</method>
	</class>
	
	<class name="Asset">
		<attr name="owner" type="Person" />
		<attr name="value" type="int64"/>
		<method name="sell" type="void">
			<arg name="newowner" type="Person" />
		</method>
	</class>

	<enum name="State">
		<member name="TX"/>
		<member name="NY"/>
		<member name="CA"/>
		<member name="IL"/>
	</enum>
	
	<record name="Address">
		<attr name="state" type="State"/>
		<attr name="city" type="str" />
		<attr name="street" type="str" />
		<attr name="number" type="int32" />
	</record>
	
	<class name="House" extends="Asset">
		<attr name="address" type="Address" />
	</class>

	<class name="Car" extends="Asset">
		<attr name="hand" type="int32" />
	</class>
	
</service>













